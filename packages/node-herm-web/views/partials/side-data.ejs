<div class="accordion w-100" id="sideMenu">

    <h3 class="mb-4">Models</h3>

    <% 
        let models = [];
        if (data) {
            models = data.models;
        }
        models.forEach((model, i) => { 
            const heading = `heading${i}`;
    %>

    <div class="card">
        <div class="card-header" id="<%= heading %>">
            <h5 class="mb-0">
                <button class="btn btn-link" type="button" data-toggle="collapse" data-target="<%= `#collapse${i}` %>"
                    aria-expanded="true" aria-controls="<%= `#collapse${i}` %>">
                    <%= model.name %>
                </button>
            </h5>
        </div>

        <div id="<%= `collapse${i}` %>" class="collapse show" aria-labelledby="<%= heading %>" data-parent="#sideMenu">
            <ul class="list-group list-group-flush mb-4">
                <% model.properties.forEach(property => { %>
                <li class="list-group-item d-flex justify-content-between">
                    <div><%= property.name %></div>

                    <div class="text-capitalize text-right">
                        <%= property.type %>
                    </div>
                </li>
                <% }) %>
            </ul>
        </div>
    </div>

    <% }) %>

</div>